{
  "version": 2,
  "routes": [
    {
      "src": "/guilds/([0-9]+)/(.*)",
      "dest": "/guilds/[guild_id]/$2"
    }, {
      "src": "/guilds/([0-9]+)/vars",
      "dest": "/guilds/[guild_id]/vars/$2"
    }, {
      "src": "/guilds/([0-9]+)/vars/([^/]+)/(.*)",
      "dest": "/guilds/[guild_id]/vars/[var_name]/$2"
    },
    {
      "src": "/guilds/([0-9]+)/arts",
      "dest": "/guilds/[guild_id]/arts/$2"
    }, {
      "src": "/guilds/([0-9]+)/arts/([^/]+)/(.*)",
      "dest": "/guilds/[guild_id]/arts/[art_name]/$2"
    }
  ]
}
